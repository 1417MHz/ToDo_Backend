{
  "name": "todo-backend",
  "version": "0.0.1",
  "engine-strict": true,
  "engines": {
    "node": ">=14"
  },
  "private": true,
  "scripts": {
    "build": " rm -rf ./build && ./node_modules/typescript/bin/tsc && npm run add-assets",
    "add-assets": "copyfiles -u 1 './src/**/!(*.js|*.jsx|*.map|*.ts|*.tsx)' ./build/src",
    "local": "set NODE_ENV=test && nodemon --exec ts-node --transpile-only ./src/app.ts",
    "dev": "NODE_ENV=development ts-node --transpile-only ./src/app.ts",
    "start": "NODE_ENV=production ts-node --transpile-only ./src/app.ts",
    "lint": "eslint ./src/*/.ts --fix"
  },
  "dependencies": {
    "aws-sdk": "^2.742.0",
    "axios": "^0.20.0",
    "bcrypt": "^5.0.1",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-session": "^1.17.1",
    "express-winston": "^4.0.3",
    "fluent-logger": "^3.4.1",
    "form-data": "^3.0.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.27",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-rsa": "^1.1.1",
    "request": "^2.88.2",
    "serialize-error": "^7.0.1",
    "shortid": "^2.2.15",
    "swagger-ui-dist": "^3.33.0",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/connect-redis": "0.0.14",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.0",
    "@types/morgan": "^1.9.1",
    "@types/mysql": "^2.15.15",
    "@types/sinon": "^9.0.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "chai": "^4.2.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.10.0",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.1.3",
    "node-mocks-http": "^1.8.1",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "randexp": "^0.5.3",
    "sinon": "^9.0.2",
    "superagent": "^5.3.1",
